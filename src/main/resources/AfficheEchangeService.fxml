<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.Echange.AfficheEchangeService">
    <children>
        <TableView fx:id="exchangeTableView" layoutX="23.0" layoutY="55.0" prefHeight="290.0" prefWidth="554.0">
            <columns>
                <TableColumn fx:id="serviceInColumn" prefWidth="200.0" text="Service In" />
                <TableColumn fx:id="serviceOutColumn" prefWidth="200.0" text="Service Out" />
                <TableColumn fx:id="dateEchangeColumn" prefWidth="160.0" text="Date Echange" />
                <TableColumn fx:id="valideColumn" prefWidth="80.0" text="Valide" />
                <TableColumn fx:id="actionsColumn" prefWidth="100.0" text="Actions" />
            </columns>
        </TableView>
        <Button fx:id="idUpdateExchange" layoutX="445.0" layoutY="360.0" mnemonicParsing="false" onAction="#updateExchange" text="Update" />

        <Button fx:id="idDeleteExchange" layoutX="294.0" layoutY="357.0" mnemonicParsing="false" onAction="#deleteExchange" prefHeight="31.0" prefWidth="120.0" text="Delete" />
    </children>
</AnchorPane>
